<%- include ../layouts/cabecera.ejs %>
<h1>Datelles del productos</h1>


<div>
   
    <br>
   
    <table class="table table-striped  table-hover p-2">
        <thead>
            <tr>
               
                <th scope="col">Nombre</th>
                <th scope="col">Imagen 02</th>
                <th scope="col">Imagen 03</th>
                
            </tr>
        </thead>
        <tbody>
            <% productos.forEach(producto=>
                { %>
                <tr>
                   
                    <td>
                        <%= producto.nombre %>
                    </td>
                    <td><img class="img-thumbnail" width="150" src="/images/<%= producto.imagen2 %>" alt=""></td>
                    <td><img class="img-thumbnail" width="150" src="/images/<%= producto.imagen3 %>" alt=""></td>
                   
                  
                </tr>
                <% })%>
        </tbody>
    </table>

</div>





